{
    "name": "artificertech/blade-time-elements",
    "description": "A blade implementation of the github/time-elements package",
    "license": "MIT",
    "authors": [
        {
            "name": "Cole Shirley",
            "email": "cole.shirley@artificertech.com"
        }
    ],
    "homepage": "https://github.com/artificertech/blade-time-elements",
    "keywords": [
        "Laravel",
        "BladeTimeElements"
    ],
    "require": {
        "illuminate/support": "~7|~8|~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Artificertech\\BladeTimeElements\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artificertech\\BladeTimeElements\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Artificertech\\BladeTimeElements\\BladeTimeElementsServiceProvider"
            ],
            "aliases": {
                "BladeTimeElements": "Artificertech\\BladeTimeElements\\Facades\\BladeTimeElements"
            }
        }
    }
}
